{
    "name": "online-school-app",
    "version": "1.0.0",
    "description": "",
    "main": "build/app.js",
    "scripts": {
        "lint:hook": "npx eslint --max-warnings 0",
        "lint": "DEBUG=eslint:linter npx eslint --max-warnings 0 ./src/** ./test/**",
        "build": "rimraf build && tsc -p .",
        "test": "JEST_RUN=1 jest --watch",
        "test:nowatch": "JEST_RUN=1 jest --silent --forceExit",
        "dev": "NODE_ENV=development nodemon src/app.ts --ignore \"test/specs/**\"",
        "dc:dev": "docker-compose --env-file ./env/.env.development up",
        "dc:prod": "docker-compose -f docker-compose.prod.yml --env-file ./env/.env.production up",
        "dc:down": "docker-compose down",
        "db:migrate:dev": "NODE_ENV=development npx sequelize-cli db:migrate",
        "db:undo:dev": "NODE_ENV=development npx sequelize-cli db:migrate:undo",
        "db:seed:all": "NODE_ENV=development npx sequelize-cli db:seed:all",
        "db:seed:undo": "NODE_ENV=development npx sequelize-cli db:seed:undo:all"
    },
    "lint-staged": {
        "src/**/*.(ts)": [
            "npm run lint:hook"
        ],
        "test/**/*.(ts)": [
            "npm run lint:hook"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "api",
        "express",
        "rest api"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@faker-js/faker": "6.0.0-alpha.7",
        "@types/bcryptjs": "2.4.2",
        "@types/cron": "1.7.3",
        "@types/express": "4.17.13",
        "@types/jest": "27.4.1",
        "@types/jsonwebtoken": "8.5.8",
        "@types/lodash": "4.14.178",
        "@types/passport": "1.0.7",
        "@types/passport-http": "0.3.9",
        "@types/swagger-jsdoc": "6.0.1",
        "@types/swagger-ui-express": "4.1.3",
        "@typescript-eslint/eslint-plugin": "5.13.0",
        "@typescript-eslint/parser": "5.13.0",
        "ajv": "8.10.0",
        "axios": "0.26.0",
        "eslint": "8.10.0",
        "husky": "4.3.8",
        "jest": "27.5.1",
        "lint-staged": "12.3.4",
        "nodemon": "2.0.15",
        "rimraf": "3.0.2",
        "sequelize-cli": "6.4.1",
        "ts-jest": "27.1.3",
        "ts-node": "10.5.0",
        "typescript": "4.5.5"
    },
    "dependencies": {
        "bcryptjs": "2.4.3",
        "body-parser": "1.19.1",
        "cron": "1.8.2",
        "dotenv": "14.3.2",
        "express": "4.17.2",
        "express-validator": "6.14.0",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.21",
        "passport": "0.5.2",
        "passport-headerapikey": "1.2.2",
        "passport-http": "0.3.0",
        "pg": "8.7.1",
        "pg-hstore": "2.3.4",
        "sequelize": "6.14.1",
        "swagger-jsdoc": "6.1.0",
        "swagger-ui-express": "4.3.0",
        "winston": "3.7.2"
    }
}